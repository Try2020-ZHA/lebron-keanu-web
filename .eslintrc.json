{
	"parser": "babel-eslint",
	"env": {
		"browser": true,
		"es2020": true
	},
	"extends": ["airbnb", "plugin:react/recommended", "prettier"],
	"parserOptions": {
		"ecmaFeatures": {
			"jsx": true
		},
		"ecmaVersion": 11,
		"sourceType": "module"
	},
	"plugins": ["react"],
	"rules": {
		"react/prefer-stateless-function": [0],
		"react/jsx-indent": [2, "tab"],
		"react/jsx-indent-props": [2, "tab"],
		"import/prefer-default-export": [0]
	}
}
